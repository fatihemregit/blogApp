@model BlogApp.Models.AuthUserController.UserDetailViewModel

<p class="p-2">Kulllanıcı Detay : @Model.UserName</p>
<hr />
<div class="row p-2">
    <label asp-for="@Model.Id" class="col-md-3 fw-bold"></label>
    <p class="col-md-9">@Model.Id</p>
</div>
<div class="row p-2">
    <label asp-for="@Model.UserName" class="col-md-3 fw-bold"></label>
    <p class="col-md-9">@Model.UserName</p>
</div>
<div class="row p-2">
    <label asp-for="@Model.Email" class="col-md-3 fw-bold"></label>
    <p class="col-md-9">@Model.Email</p>
</div>
<br />
<a href="@Url.Action("GetAllUsers", "AuthUser")"> Tüm Kullanıcı listesine git</a>

